/* DESKTOP NAV                 */

/* ==========================  */


.desk-menu
  display none

@media only screen and (min-width:800px)

  .desk-menu
    // border 1px dotted blue
    display block
    margin-bottom 20px
    position relative
    li 
      width 16.6666%
      float left 
      // height 80px
      border-bottom 4px solid rgba(168, 172, 177, .3)
      position relative
      a 
        display block 
        text-align center
        white-space nowrap
        text-decoration none
        color #777A81
        padding 30px 5px
        -webkit-transition all 250ms linear
        -moz-transition all 250ms linear
        transition all 250ms linear
        transition-property #ffffff,#FF3347
      a:hover 
        color #FF3347 
      a.active
        // color blue !important
    li::after
      position absolute 
      width 1px 
      height 50px 
      right 0px 
      top 14px
      background rgba(168, 172, 177, .3)
      content ""
    li:last-of-type::after
      display none
    li.active 
      border-bottom 4px solid #ff3347
      -webkit-transition all 250ms linear
      -moz-transition all 250ms linear
      transition all 250ms linear
      transition-property 1,#FF3347
      a
        color #ff3347






