      a 
        padding-left 50px
        display block

  #slide
    position fixed
    width 100%
    height 100px
    top 0px
    background red
    height 0px

   

  .animate-slide 
    background white
    position fixed
    z-index 50
    width  100%
    height 100%
    top  0px
    -webkit-transform  translate3d(0,0,0)
    transform  translate3d(0,0,0,)
    -webkit-backface-visibility  hidden
    backface-visibility  hidden
    -webkit-perspective  1000
    perspective  1000


  .animate-slide.ng-hide-add,
  .animate-slide.ng-hide-remove 
    display block !important


  .animate-slide.ng-hide-remove.ng-hide-remove-active 
    -webkit-animation 0.3s slide-down
    animation 0.3s slide-down


  .animate-slide.ng-hide-add.ng-hide-add-active 
    -webkit-animation 0.3s 
    animation 0.3s slide-up


  .animate-slide.ng-hide 
    top -100%
    display block !important


  /* Chrome, Safari, Opera */
  @-webkit-keyframes slide-down
  {
    0%     {top:-100;}
    100%   {top:0%;}
  }

  /* Standard syntax */
  @keyframes slide-down
  {
    0%     {top:-100%;}
    100%   {top:0%;}
  }


  /* Chrome, Safari, Opera */
  @-webkit-keyframes slide-up
  {
    0%   {top:0%;}
    100%  {top:-100%;}
  }

  @keyframes slide-up
  {
    0%   {top: 0%;}
    100%  {top: -100%;}
  }






